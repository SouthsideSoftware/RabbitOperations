{
  "AppSettings": {
    "LogLevel": "Debug",
    "LogFile":  "/logs/RabbitOperations.Collector/log-{Date}.log",
    "AllowDevelopmentMode": "true",
    "LogInRavenDbExpirationTimeSpan": "1.00:00:00",
    "LogErrorInRavenDbExpirationTimeSpan": "-00:00:00.0010000"
  },
  "RavenDbSettings": {
    "UseEmbedded": "true",
    "DefaultTenant": "RabbitOperations",
    "EmbeddedRavenDb": {
      "ManagementPort": "8081",
      "DataDirectory": "c:/data/RabbitOperations"
    },
    "ExternalRavenDb": {
      "ExteralUrl": "http://localhost:8080"
    }
  }
}
